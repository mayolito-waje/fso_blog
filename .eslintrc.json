{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": ["airbnb-base", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "ignorePackages"
      }
    ],
    "no-underscore-dangle": "off"
  }
}
